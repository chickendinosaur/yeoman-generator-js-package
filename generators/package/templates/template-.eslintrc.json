{
	"extends": ["airbnb-base"],
	"root": true,
	"env": {
		"browser": true,
		"node": true
	},
	"ext": [
		".js",
		".jsx",
		".html"
	],
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"plugins": [
		"import",
		"html"
	],
	"rules": {}
}
